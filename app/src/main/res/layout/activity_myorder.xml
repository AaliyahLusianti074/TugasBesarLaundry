<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg2">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="22dp"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/home"
            android:onClick="masukmenu"/>

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/jadwal" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/notification" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/user"/>
    </LinearLayout>


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="283dp"
        android:layout_height="77dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        app:srcCompat="@drawable/logo" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="361dp"
        android:layout_height="89dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="90dp"
        app:srcCompat="@drawable/judul" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="370dp"
        android:layout_height="163dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="160dp"
        app:srcCompat="@drawable/rincian" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="345dp"
        android:layout_height="156dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="310dp"
        app:srcCompat="@drawable/tangga" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="214dp"
        android:layout_height="33dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="465dp"
        android:onClick="antarLaundry"
        app:srcCompat="@drawable/antar" />
    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="214dp"
        android:layout_height="33dp"
        android:layout_marginTop="465dp"
        android:layout_marginLeft="185dp"
        android:onClick="ambilSendiri"
        app:srcCompat="@drawable/ambil" />


</RelativeLayout>